{
  "name": "@delivc/uikit",
  "version": "1.0.4",
  "description": "delivc react UI KIT based upon evergreen",
  "author": "juliankoehn",
  "license": "MIT",
  "repository": "https://github.com/delivc/uikit",
  "main": "commonjs/index.js",
  "module": "esm/index.js",
  "unpkg": "umd/uikit.js",
  "types": "index.d.ts",
  "files": [
    "commonjs",
    "esm",
    "umd",
    "index.d.ts"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "xo && ava",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build-commonjs": "BABEL_ENV=commonjs babel src --out-dir commonjs --ignore '**/stories','**/test' --source-maps inline --extensions .ts,.tsx",
    "build-esm": "BABEL_ENV=esm babel src --out-dir esm --ignore '**/stories','**/test' --source-maps inline --extensions .ts,.tsx",
    "build-umd": "rollup -c",
    "build": "concurrently --names 'commonjs,esm,umd' 'yarn build-commonjs' 'yarn build-esm' 'yarn build-umd'",
    "release": "np",
    "prepare": "yarn run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/runtime": "^7.1.2",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "evergreen-ui": "^4.23.3",
    "react-hook-form": "^5.1.1",
    "react-scripts": "^3.4.0"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@blueprintjs/icons": "^3.14.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-info": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/preset-create-react-app": "^2.0.0",
    "@storybook/react": "^5.3.17",
    "@svgr/rollup": "^2.4.1",
    "@types/jest": "^23.1.5",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/yup": "^0.26.33",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "ava": "^3.5.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^7.1.5",
    "babel-runtime": "^6.26.0",
    "concurrently": "^5.1.0",
    "cross-env": "^5.1.4",
    "css-loader": "^3.4.2",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-config-xo-typescript": "^0.26.0",
    "eslint-plugin-react": "^7.19.0",
    "node-sass": "^4.13.1",
    "np": "^6.2.0",
    "prettier": "^1.19.1",
    "react": "^16.4.1",
    "react-docgen-typescript-loader": "^3.7.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "^2.16.0",
    "rollup": "^1.29.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.17.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "xo": "^0.28.0",
    "yup": "^0.28.3"
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": [
      "xo-typescript",
      "xo-react",
      "prettier",
      "prettier/react"
    ],
    "envs": [
      "browser"
    ],
    "space": true,
    "semicolon": false,
    "rules": {
      "indent": [
        "off"
      ],
      "react/jsx-indent-props": [
        "off"
      ],
      "react/jsx-indent": [
        "off"
      ],
      "react/require-default-props": [
        "off"
      ],
      "react/default-props-match-prop-types": [
        "off"
      ],
      "react/forbid-component-props": [
        "off"
      ],
      "react/jsx-no-bind": [
        "off"
      ],
      "react/jsx-sort-props": [
        "off"
      ],
      "unicorn/filename-case": [
        "off"
      ],
      "unicorn/prefer-node-append": [
        "off"
      ],
      "unicorn/prefer-string-slice": [
        "warn"
      ],
      "unicorn/prefer-node-remove": [
        "off"
      ],
      "unicorn/prefer-dataset": [
        "off"
      ],
      "unicorn/catch-error-name": [
        "off"
      ]
    },
    "settings": {
      "react": {
        "version": "^16.3.0"
      }
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "*.js": [
      "xo --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
